doctype html
html
  head
    title!= title
  body
    each val, index in messages
      div.flash= val
    form(method="post", action="/signin")
      div
        label Username:
        input(type="text", name="username")
      div
        label Password:
        input(type="password", name="password")
      div
        input(type="submit", name="Sing up")
